/**
 * angular js module
 */

var app=angular.module("myApp",['ngroute'])
app.config(function($routeProvider){
	$routeProvider
	.when('/registration',{
		templateUrl:'views/registrationform.html',
		controller:'UserController'
	})
	
	.when('/login',{
		templateUrl:'views/login.html',
		controller:'UserController'
	})
	.otherwise({
		templateUrl:'views/home.html'
	})
})